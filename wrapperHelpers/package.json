{
	"name": "@spookelton/wrapperHelpers",
	"version": "1.0.0",
	"description": "Shared types used for serverWrapper & modules",
	"main": "./dist",
	"exports": {
		"./*": "./dist/*.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/*"
			]
		}
	},
	"scripts": {
		"tsc": "tsc && copy /Y package.json dist",
		"send": "npm run tsc && cd ./dist && npm publish",
