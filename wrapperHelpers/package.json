{
	"name": "@spookelton/wrapperhelpers",
	"version": "1.0.0",
	"description": "Shared types used for serverWrapper & modules",
	"exports": {
		"./*": "./dist/*.js"
	},
	"typesVersions": {
		"*": {
			"colors": [
				"dist/colors"
			],
			"types": [
				"dist/types"
			],
			"modul": [
				"dist/modul"
			]
		}
	},
	"scripts": {
		"tsc": "tsc && copy /Y package.json dist",
		"send": "npm run tsc && cd ./dist && npm publish",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Spookelton/wrapperTypes.git"
	},
	"author": "Inrix",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Spookelton/wrapperTypes/issues"
	},
	"devDependencies": {
		"@inrixia/helpers": "^1.22.1",
		"discord.js": "^13.1.0"
	}
}
